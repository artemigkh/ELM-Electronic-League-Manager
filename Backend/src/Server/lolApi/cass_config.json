{
  "global": {
    "version_from_match": "patch",
    "default_region": "NA"
  },
  "plugins": {},
  "pipeline": {
    "Cache": {},
    "DDragon": {},
    "RiotAPI": {
      "api_key": "RGAPI-fae8b720-c612-4271-82b3-ef52d57362f2",
      "request_error_handling": {
        "404": {
          "strategy": "throw"
        },
        "429": {
          "service": {
            "strategy": "exponential_backoff",
            "initial_backoff": 1.0,
            "backoff_factor": 2.0,
            "max_attempts": 4
          },
          "method": {
            "strategy": "retry_from_headers",
            "max_attempts": 5
          },
          "application": {
            "strategy": "retry_from_headers",
            "max_attempts": 5
          }
        },
        "500": {
          "strategy": "exponential_backoff",
          "initial_backoff": 1.0,
          "backoff_factor": 2.0,
          "max_attempts": 999
        },
        "503": {
          "strategy": "exponential_backoff",
          "initial_backoff": 1.0,
          "backoff_factor": 2.0,
          "max_attempts": 999
        },
        "timeout": {
          "strategy": "throw"
        },
        "403": {
          "strategy": "throw"
        }
      }
    }
  },
  "logging": {
    "print_calls": true,
    "print_riot_api_key": false,
    "default": "WARNING",
    "core": "WARNING"
  }
}